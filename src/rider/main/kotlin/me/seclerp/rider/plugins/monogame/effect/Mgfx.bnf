{
  parserClass="me.seclerp.rider.plugins.monogame.effect.parser.EffectParser"

  extends="com.intellij.extapi.psi.ASTWrapperPsiElement"

  psiClassPrefix="Effect"
  psiImplClassSuffix="Impl"
  psiPackage="me.seclerp.rider.plugins.monogame.effect.psi"
  psiImplPackage="me.seclerp.rider.plugins.monogame.effect.psi"

  elementTypeHolderClass="me.seclerp.rider.plugins.monogame.effect.psi.EffectTypes"
  elementTypeClass="me.seclerp.rider.plugins.monogame.effect.psi.EffectElementType"
  tokenTypeClass="me.seclerp.rider.plugins.monogame.effect.psi.EffectTokenType"
}

mgcbFile ::= item_*

private item_ ::= (
    option
    | set_instruction
    | if_instruction
    | COMMENT
    | WHITE_SPACE
)

option ::= (
    OPTION_KEY OPTION_SEPARATOR OPTION_VALUE
    | OPTION_KEY OPTION_SEPARATOR
    | OPTION_KEY
)

set_instruction ::= (
    SET_KEYWORD WHITE_SPACE PREPROCESSOR_IDENTIFIER EQ PREPROCESSOR_VALUE
    | SET_KEYWORD WHITE_SPACE PREPROCESSOR_IDENTIFIER
)

if_instruction ::= (
    IF_KEYWORD WHITE_SPACE PREPROCESSOR_IDENTIFIER EQ PREPROCESSOR_VALUE WHITE_SPACE item_* ENDIF_KEYWORD
    | IF_KEYWORD WHITE_SPACE PREPROCESSOR_IDENTIFIER WHITE_SPACE item_* ENDIF_KEYWORD
)